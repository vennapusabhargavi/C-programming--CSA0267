//armstrong number
#include <stdio.h>
#include <math.h>
int isArmstrong(int n);
int main() {
    int start, end;
    printf("Enter start and end intervals: ");
    scanf("%d %d", &start, &end);
    printf("Armstrong numbers between %d and %d are: ", start, end);
    for (int i = start; i <= end; i++) {
        if (isArmstrong(i)) {
            printf("%d ", i);
        }
    }
    return 0;
}
int isArmstrong(int n) {
    int num = n, digitCount = 0, sum = 0;
    while (num > 0) {
        digitCount++;
        num /= 10;
    }
    num = n;
    while (num > 0) {
        int digit = num % 10;
        sum += pow(digit, digitCount);
        num /= 10;
    }
    return sum == n;
}
