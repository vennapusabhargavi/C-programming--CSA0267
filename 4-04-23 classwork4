//write a programm to find the largest of two numbers 
#include <stdio.h>
int main()
{
	int num1,num2;
	printf("Enter the first value :");
	scanf("%d",&num1);
	printf("Enter the second value :");
	scanf("%d",&num2);
	if(num1>num2)
	{
		printf("%d is the biggest number ",num1);
	}
	else
	{
		printf("%d is the biggest number",num2);
	}
}
